[Desktop Entry]
Name=HexChat(Over TOR)
GenericName=IRC Client(Over TOR)
Comment=Chat with other people online
Keywords=IM;Chat;
Exec=torsocks hexchat %U
Icon=hexchat
Terminal=false
Type=Application
Categories=GTK;Network;IRCClient;
StartupNotify=true
X-GNOME-UsesNotifications=true
MimeType=x-scheme-handler/irc;x-scheme-handler/ircs;
Actions=SafeMode;

Name[en]=HexChat(Over TOR)

[Desktop Action SafeMode]
Name=Open Safe Mode
Exec=hexchat --no-auto --no-plugins
